package br.com.estudo.rest;

import javax.ws.rs.GET;
import javax.ws.rs.Path;

@Path("/hello");
public class HelloResource {
	@GET
	public String get() {
		return "HTTP GET";
	}
	@POST
	public String post() {
		return "HTTP POST";
	}
	@PUT
	public String put() {
		return "HTTP PUT";
	}
	@DELETE
	public String delete() {
		return "HTTP DELETE";
	}
	
}
